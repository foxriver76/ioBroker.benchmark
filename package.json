{
  "name": "iobroker.benchmark",
  "version": "1.2.0",
  "description": "Benchmark your system",
  "author": {
    "name": "Moritz Heusinger",
    "email": "moritz.heusinger@gmail.com"
  },
  "homepage": "https://github.com/foxriver76/ioBroker.benchmark",
  "license": "MIT",
  "keywords": [
    "system",
    "benchmark"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:foxriver76/ioBroker.benchmark.git"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.1.5",
    "@types/pidusage": "^2.0.2",
    "pidusage": "^3.0.0",
    "promisify-child-process": "^4.1.1",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.2",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.1",
    "@iobroker/testing": "^4.1.3",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.16",
    "@types/proxyquire": "^1.3.28",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "axios": "^1.6.8",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "mocha": "^10.4.0",
    "prettier": "^2.6.2",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/",
    "release": "release-script -p iobroker"
  },
  "bugs": {
    "url": "https://github.com/foxriver76/ioBroker.benchmark/issues"
  },
  "readmeFilename": "README.md"
}
